{
  "name": "Pages to Folders",
  "description": "Sketch plugin that exports all the artboards of a Sketch file into folders, which are based on the pages of the Sketch file.",
  "author": "Ivan Bozic",
  "homepage": "http://github.com/ivanbozic/sketch-pages-to-folders",
  "version": 0.2,
  "identifier": "com.ivanbozic.sketch.pages-to-folders",
  "compatibleVersion": 42,
  "bundleVersion": 1,
  "commands": [
    {
      "name": "Export All Pages",
      "identifier": "export-all",
      "script": "export.js",
      "handler": "exportAllPages"
    }
  ],
  "menu": {
    "title": "Pages to Folders",
    "items": [
      "export-all"
    ]
  }
}